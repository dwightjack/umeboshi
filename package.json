{
  "name": "umeboshi",
  "private": true,
  "author": "Marco Solazzi @dwightjack",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ],
    "nohoist": [
      "eslint"
    ]
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "bugs": "https://github.com/dwightjack/umeboshi/issues",
  "keywords": [
    "umeboshi"
  ],
  "scripts": {
    "test": "jest",
    "bundle": "lerna run bundle"
  },
  "devDependencies": {
    "documentation": "~6.3.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^22.0.0",
    "lerna": "^3.4.3",
    "microbundle": "^0.6.0",
    "prettier": "^1.14.3"
  },
  "jest": {
    "verbose": true,
    "bail": false,
    "collectCoverageFrom": [
      "**/packages/umeboshi-*/**/*.js",
      "**/packages/eslint-*/**/*.js",
      "**/packages/babel-*/**/*.js",
      "!**/bin/**",
      "!**/__mocks__/**",
      "!**/__tests__/**"
    ],
    "collectCoverage": false,
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@vue/test-utils": "~1.0.0-beta.25",
    "css-loader": "^0.28.0",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "stylelint": "^9.0.0",
    "vue": "^2.5.0",
    "vue-template-compiler": "^2.5.0",
    "webpack-bundle-analyzer": "^2.0.0",
    "webpack": "^4.8.3"
  }
}
