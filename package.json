{
  "name": "umeboshi",
  "private": true,
  "author": "Marco Solazzi @dwightjack",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": ">=6.9.0"
  },
  "bugs": "https://github.com/dwightjack/umeboshi/issues",
  "keywords": [
    "umeboshi"
  ],
  "scripts": {
    "test": "jest",
    "bundle": "lerna run bundle"
  },
  "devDependencies": {
    "documentation": "~6.3.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.0.0",
    "lerna": "^3.8.0",
    "microbundle": "^0.9.0",
    "prettier": "^1.15.3"
  },
  "jest": {
    "verbose": true,
    "bail": false,
    "collectCoverageFrom": [
      "**/packages/umeboshi-*/**/*.js",
      "**/packages/eslint-*/**/*.js",
      "**/packages/babel-*/**/*.js",
      "!**/bin/**",
      "!**/__mocks__/**",
      "!**/__tests__/**"
    ],
    "collectCoverage": false,
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@vue/test-utils": "~1.0.0-beta.27",
    "css-loader": "^2.0.1",
    "enzyme": "~3.8.0",
    "enzyme-adapter-react-16": "~1.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "stylelint": "^9.9.0",
    "vue": "^2.5.21",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
